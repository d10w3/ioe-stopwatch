{
  "name": "voice-analytics-ui",
  "version": "1.10.0",
  "description": "IO Education Test",
  "main": "index.js",
  "scripts": {
    "test": "karma start karma.conf.js --single-run",
    "test-alive": "karma start karma.conf.js",
    "lint": "eslint --fix js"
  },
  "dependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bluebird": "^2.9.34",
    "bootstrap": "^3.3.4",
    "bootstrap-datepicker": "^1.4.0",
    "browser-filesaver": "^1.1.0",
    "bundle-loader": "^0.5.0",
    "clipboard": "^1.5.5",
    "config": "^1.14.0",
    "core-js": "^0.9.18",
    "css-loader": "^0.15.6",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "file-loader": "^0.7.2",
    "font-awesome": ">=4.2.0",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.5.2",
    "jquery": "~2.1.1",
    "json-loader": "0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^3.9.3",
    "moment": "^2.9.0",
    "moment-timezone": "^0.4.1",
    "react": "^0.14.0",
    "react-bootstrap": "^0.29.5",
    "react-bootstrap-date-picker": "^3.1.0",
    "react-bootstrap-switch": "^3.4.5",
    "react-dom": "^0.14.0",
    "react-router": "^2.0.0-rc5",
    "react-wavesurfer": "^0.5.2",
    "rewire": "^2.5.1",
    "rewire-webpack": "^1.0.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.11"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "istanbul-instrumenter-loader": "0.1.3",
    "jasmine": "2.3.1",
    "jasmine-core": "2.3.4",
    "jquery-mockjax": "2.0.1",
    "karma": "0.13.9",
    "karma-chrome-launcher": "0.2.0",
    "karma-cli": "0.0.4",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "karma-phantomjs-launcher": "0.2.0",
    "karma-webpack": "1.7.0",
    "mocha": "^2.3.3",
    "node-libs-browser": "^0.5.2",
    "phantomjs": "1.9.17",
    "react-hot-loader": "^1.2.7",
    "webpack-dev-server": "^1.6.5",
    "precommit-hook-eslint": "^3.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": 1
    },
    "globals": {
      "window": true,
      "document": true,
      "require": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-multi-spaces": 2,
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 2,
      "curly": [
        2,
        "all"
      ],
      "eqeqeq": 0,
      "camelcase": 0,
      "no-unused-vars": 1,
      "no-underscore-dangle": 0,
      "strict": 1,
      "indent": [
        2,
        2
      ],
      "no-var": 1,
      "no-loop-func": 0,
      "use-isnan": 2,
      "no-alert": 2,
      "yoda": 2,
      "no-shadow": 2,
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "comma-style": [
        2,
        "last"
      ],
      "react/jsx-boolean-value": 1,
      "react/jsx-curly-spacing": 1,
      "react/jsx-key": 1,
      "react/jsx-no-duplicate-props": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-pascal-case": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-danger": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-direct-mutation-state": 1,
      "react/no-multi-comp": 1,
      "react/no-unknown-property": 1,
      "react/react-in-jsx-scope": 1,
      "react/require-extension": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1,
      "react/wrap-multilines": 1
    },
    "ecmaFeatures": {
      "jsx": true,
      "blockBindings": true,
      "arrowFunctions": true,
      "forOf": true
    },
    "plugins": [
      "react"
    ]
  }

}
